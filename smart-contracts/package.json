{
  "name": "laxce-dex-contracts",
  "version": "1.0.0",
  "description": "9-Layer DEX Smart Contracts for LAXCE",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:core": "npx hardhat test test/01-core/**/*.js",
    "test:token": "npx hardhat test test/02-token/**/*.js",
    "test:pool": "npx hardhat test test/03-pool/*.test.js",
    "test:security": "npx hardhat test test/09-security/**/*.js",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:testnet": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy.js --network mainnet",
    "deploy:core": "npx hardhat run scripts/deploy-core.js --network localhost",
    "deploy:core:testnet": "npx hardhat run scripts/deploy-core.js --network sepolia",
    "deploy:token": "npx hardhat run scripts/deploy-tokens.js --network localhost",
    "deploy:token:testnet": "npx hardhat run scripts/deploy-tokens.js --network sepolia",
    "deploy:pool": "npx hardhat run scripts/deploy-pool.js --network localhost",
    "deploy:pool:testnet": "npx hardhat run scripts/deploy-pool.js --network sepolia",
    "deploy:security": "npx hardhat run scripts/deploy-security.js --network localhost",
    "deploy:security:testnet": "npx hardhat run scripts/deploy-security.js --network sepolia",
    "test:router": "npx hardhat test test/04-router/*.test.js",
    "deploy:router": "npx hardhat run scripts/deploy-router.js --network localhost",
    "deploy:router:testnet": "npx hardhat run scripts/deploy-router.js --network sepolia",
    "test:oracle": "npx hardhat test test/05-oracle/*.test.js",
    "deploy:oracle": "npx hardhat run scripts/deploy-oracle.js --network localhost",
    "deploy:oracle:testnet": "npx hardhat run scripts/deploy-oracle.js --network sepolia",
    "test:quoter": "npx hardhat test test/06-quoter/*.test.js",
    "deploy:quoter": "npx hardhat run scripts/deploy-quoter.js --network localhost",
    "deploy:quoter:testnet": "npx hardhat run scripts/deploy-quoter.js --network sepolia",
    "verify": "npx hardhat verify",
    "clean": "npx hardhat clean",
    "coverage": "npx hardhat coverage",
    "lint": "solhint 'contracts/**/*.sol'",
    "flatten": "npx hardhat flatten",
    "node": "npx hardhat node"
  },
  "keywords": [
    "dex",
    "defi",
    "smart-contracts",
    "solidity",
    "ethereum",
    "polygon",
    "bsc"
  ],
  "author": "LAXCE Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "chai": "^4.2.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.1",
    "typechain": "^8.3.0"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.8.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "dotenv": "^16.0.0"
  }
}
